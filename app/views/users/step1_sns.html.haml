= form_with model:@user, url: step2_users_path,local: true,method: :get do |f|
  = f.text_field :name,placeholder: "ニックネーム",value: session[:name]
  = f.email_field :email,placeholder: "メール",value: session[:email]

  = f.text_field :family_name,placeholder: "苗字"
  = f.text_field :first_name,placeholder: "名前"
  = f.text_field :first_name_kana,placeholder: "苗字カナ"
  = f.text_field :family_name_kana,placeholder: "名前カナ"
  = f.date_select(:birth_day,start_year: 1930, end_year: (Time.now.year - 18), default: Date.new(1989, 1, 1))
  = f.submit "次"

=  render "devise/shared/links"